2d_physics/layer_2="Player"
2d_physics/layer_3="Player_Hurtbox"
2d_physics/layer_4="Enemy_Hurtbox"
2d_physics/layer_5="Enemy"
2d_physics/layer_6="SoftCollisions"

[memory]

limits/command_queue/multithreading_queue_size_kb=1024

[physics]

common/physics_fps=120
common/physics_jitter_fix=1.0
2d/physics_engine="GodotPhysics"
2d/thread_model=2

[rendering]

quality/driver/driver_name="GLES2"
quality/driver/fallback_to_gles2=true
