vram_compression/import_bptc=true
vram_compression/import_etc=true
quality/shadows/filter_mode=2
quality/shading/force_vertex_shading=true
quality/filters/use_nearest_mipmap_filter=true
2d/options/ninepatch_mode=0
batching/parameters/max_join_item_commands=32
gles2/compatibility/enable_high_float.Android=true
quality/filters/msaa=4
quality/filters/use_fxaa=true
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
quality/subsurface_scattering/quality=2
quality/2d/use_nvidia_rect_flicker_workaround=true
quality/2d/use_pixel_snap=true
